{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/register/page.jsx", "app/layout.jsx", "app/page.jsx", "app/matched-images/page.jsx", "app/matched-images/loading.jsx", "components/header.jsx", "components/image-modal.jsx", "components/partner-scroll.jsx", "components/selfie-capture.jsx", "components/theme-provider.jsx", "lib/utils.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
